@font-face {
    font-family: 'icomoon';
    src:    url('fonts/icomoon.eot?bdbh45');
    src:    url('fonts/icomoon.eot?bdbh45#iefix') format('embedded-opentype'),
        url('fonts/icomoon.ttf?bdbh45') format('truetype'),
        url('fonts/icomoon.woff?bdbh45') format('woff'),
        url('fonts/icomoon.svg?bdbh45#icomoon') format('svg');
    font-weight: normal;
    font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'icomoon' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;

    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-pinterest:before {
    content: "\e901";
}
.icon-twitter:before {
    content: "\e902";
}
.icon-facebook:before {
    content: "\e900";
}
.icon-instagram:before {
    content: "\ea92";
}
.icon-wordpress:before {
    content: "\eab4";
}


.diferent-font
	font-size 1.5em !important

.ImgBackground
	width 3000px
	height 100vh
	overflow hidden
	&-img
		max-height 100vh
		min-height 100vh
		vertical-align top
	&-scroll
		overflow-y scroll
.header
	position fixed
	// background linear-gradient(to top,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.4) 80%)
	top 0
	height 50px
	width 100%
	text-align right
	span
		margin 21px
		line-height 50px
		color white
		font-size 2em
	a
		text-decoration none
.footer
	width 100vw
	background linear-gradient(to bottom,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.4) 80%)
	color white
	position fixed
	bottom 0
	&-division
		display inline-block
		width 50%
		text-align center
		vertical-align middle
		margin 21px 0
	&-logo
		img
			height 50px